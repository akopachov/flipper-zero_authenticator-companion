{
	"plugins": ["prettier-plugin-svelte"],
	"svelteSortOrder": "scripts-markup-styles-options",
	"htmlWhitespaceSensitivity": "ignore",
	"trailingComma": "all",
	"requirePragma": false,
	"bracketSpacing": true,
	"bracketSameLine": true,
	"singleQuote": true,
	"printWidth": 120,
	"useTabs": false,
	"tabWidth": 2,
	"semi": true,
	"arrowParens": "avoid"
}
